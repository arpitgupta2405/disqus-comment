<section class="faq-section">
  <div class="container">
    <h1>Admin Panel</h1>
    <div class="faq-content" id="signIn">
      <div class="faq-inner-content">
        <h4>Sign In</h4>
        <div class="contact-sec">
          <ul>
            <li>
              <input type="email" id="email" required placeholder="Email ID*">
              <span class='error-msg hide' id="error-msg-email"></span>
            </li>
          </ul>
          <ul>
            <li>
              <input type="password" id="password" required placeholder="Password*">
              <span class='error-msg hide' id="error-msg-password"></span>
            </li>
          </ul>
        </div>
        <button type="submit" class="btn btn-primary send-msg-btn" onclick="adminSignIn(this)">Sign In</button>
        <div class="contact-us-success" id="sign-in-block" style="display:none;">
          <div class="model-success">
            <h2>Thanks for contacting us, our team will get back to you within 6 hours.</h2>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<button class="scrolltop-btn">
  <i class="fas fa-chevron-up"></i>
</button>
